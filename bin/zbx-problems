#!/usr/bin/env bash
set -euo pipefail
SD="$(CDPATH= cd -- "$(dirname -- "$0")" && pwd)"; PATH="$SD:$PATH"
. "$SD/log-lib"; . "$SD/zbx-lib"
jq -n '{recent:"true", sortfield:"eventid", sortorder:"DESC", output:"extend", selectAcknowledges:"extend", selectTags:"extend"}' | zbx_call problem.get | jq -r '.result[] | [.eventid,.name,.severity,.acknowledged,.clock]|@tsv'
